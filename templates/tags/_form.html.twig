<div class="container pt-5">
	{{ form_start(form) }}
	<div class="d-flex flex-column d-grid gap-2">
		<div class="">
			{{ form_errors(form.name) }}
		</div>
		<div class="d-flex flex-row d-grid gap-2">
			<div class="">
				<p>Le nom du tag</p>
			</div>
			<div class="">
				{{ form_widget(form.name) }}
			</div>
		</div>
	</div>
	<div class="">
		<button class="btn">{{ button_label|default('Save') }}</button>
	</div>
	{{ form_end(form) }}
</div>
